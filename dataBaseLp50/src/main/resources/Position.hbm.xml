<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.utbm.databaselp50.core.entity">
   <class name="Position" table="POSITION">
    <id column="id" name="id">
      <generator class="identity"/>
    </id>
    <property column="position_x" name="position_x" not-null="true"/>
     <property column="position_y" name="position_y" not-null="true"/>
     <property column="name" name="name" not-null="true"/>
  </class>
</hibernate-mapping>
